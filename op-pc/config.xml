<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ohio.platformer" version="v0.09.23" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ohio platformer</name>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="cordova-plugin-screen-orientation" version="3.0.2" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />

        <icon src="www/5fd613715aa0cd3ff316fb37ca0f2eba998d80b7f61ab01ebf8ff6644826de6c-ohio platformer game icon 36x36.png" density="ldpi" />
<icon src="www/52e5ec2ea85b606432e2ed3aca3ed6c23021dd2448a19c0aa3ae77bc410de83f-ohio platformer game icon 72x72.png" density="hdpi" />
<icon src="www/23982f0c5e7982199f69d644908c8fa5197290ec2576916d81ecf5721a4f20e9-ohio platformer game icon 192x192.png" density="xxxhdpi" />
<preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/d618b6a4a773e9ed849490d3e27ea426b5b990203298b9647da83d3d0979c935-ohio platformer game icon 512x512.png" />

        <preference name="AndroidWindowSplashScreenBackground" value="#000000" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />

        <preference name="SwiftVersion" value="5.3" />

        <icon src="www/52e5ec2ea85b606432e2ed3aca3ed6c23021dd2448a19c0aa3ae77bc410de83f-ohio platformer game icon 72x72.png" width="72" height="72" />
<icon src="www/f004879a7a3ffe73d4a56ef278b43ebe7f48711ef0318767b3ef7c1a721c5e6d-ohio platformer game icon 1024x1024.png" width="1024" height="1024" />

    </platform>

    <preference name="AndroidXEnabled" value="true" />

    <preference name="BackgroundColor" value="0xff000000"/>

    <!-- Android Fullscreen -->
    <preference name="Fullscreen" value="true" />

    <!-- Cordova/Phonegap version -->
    <preference name="phonegap-version" value="cli-9.0.0" />

    <plugin name="cordova-plugin-vibration" spec="3.1.1">
</plugin><plugin name="cordova-plugin-inappbrowser">
</plugin>
</widget>
