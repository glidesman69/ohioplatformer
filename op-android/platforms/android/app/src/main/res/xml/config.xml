    <icon density="xxxhdpi" src="www/23982f0c5e7982199f69d644908c8fa5197290ec2576916d81ecf5721a4f20e9-ohio platformer game icon 192x192.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="com.ionicframework.cordova.webview.IonicWebViewEngine" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="BackgroundColor" value="0xff000000" />
    <preference name="Fullscreen" value="true" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="AndroidWindowSplashScreenAnimatedIcon" value="www/d618b6a4a773e9ed849490d3e27ea426b5b990203298b9647da83d3d0979c935-ohio platformer game icon 512x512.png" />
    <preference name="AndroidWindowSplashScreenBackground" value="#000000" />
</widget>
